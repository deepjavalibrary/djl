{
  "name": "djl-ui",
  "version": "0.0.1",
  "description": "Deep Java Library UI",
  "main": "server.js",
  "repository": "https://github.com/awslabs/djl/.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.prod.js",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/victory": "^33.1.4",
    "@types/webpack": "^4.41.6",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^7.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.0",
    "react": "^16.11.0",
    "react-docgen-typescript-loader": "^3.2.1",
    "react-dom": "^16.11.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^4.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.0.3",
    "@patternfly/react-icons": "^3.15.3",
    "@patternfly/react-styles": "^3.7.4",
    "@patternfly/react-charts": "^5.3.18",
    "express": "^4.13.4",
    "react-axe": "^3.4.1",
    "react-router-dom": "^5.0.1",
    "react-router-last-location": "^2.0.1",
    "axios": "^0.19.2",
    "vertx3-eventbus-client": "^3.9.0"
  }
}
